# Database Configuration
POSTGRES_DB=geohod_dev
POSTGRES_USER=geohod_dev_user
POSTGRES_PASSWORD=dev_password_123

# Application Configuration (Development Bot)
TELEGRAM_BOT_TOKEN=DEV_TELEGRAM_BOT_TOKEN_HERE
TELEGRAM_BOT_USERNAME=DEV_BOT_USERNAME_HERE

# CORS Configuration
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,http://127.0.0.1:3000

# Link Templates
GEOHOD_CREATED_EVENT_LINK_TEMPLATE=http://localhost:3000/event/{eventId}
GEOHOD_REVIEW_LINK_TEMPLATE=http://localhost:3000/review/{userId}

# Spring Profile (use optimized settings by default)
SPRING_PROFILES_ACTIVE=dev-optimized

# Container Image Tag (set by CI/CD, can be overridden for manual deployments)
# For CI/CD deployments: IMAGE_TAG=geohod-backend:dev-abc1234
# If not set, compose file will use default: geohod-backend:dev-latest
IMAGE_TAG=geohod-backend:dev-latest